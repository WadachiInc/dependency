{
	"name": "fuelphp/dependency",
	"description": "Fuel Dependency package based on League\\Container",
	"keywords": ["dependency", "injection", "dependencies"],
	"homepage": "https://github.com/fuelphp/dependency",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4",
		"league/container": "~1.2"
	},
	"require-dev": {
		"codeception/codeception": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Dependency\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	}
}
